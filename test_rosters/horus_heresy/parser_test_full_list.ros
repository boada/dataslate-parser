            <profile id="ca37-291f-af06-a390::873a-d709-f16a-2289::1578-d22a-060c-4700" name="Machine Spirit" publicationId="ca571888--pubN99753" page="132" hidden="false" typeId="57617267656172204974656d23232344415441232323" typeName="Wargear Item">
