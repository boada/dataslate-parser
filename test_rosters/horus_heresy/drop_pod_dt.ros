            <selection id="3816-c8da-c9a7-735c" name="Playtest Rules On" entryId="7d9f-d54b-09db-e954::dace-8f0f-e696-8179" entryGroupId="7d9f-d54b-09db-e954::07c4-d0fe-fd44-2d95" number="1" type="upgrade">
