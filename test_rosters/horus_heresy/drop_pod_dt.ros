        <selection id="bd2c-17af-48b1-2325" name="Tactical Squad, Legion , Compulsory" entryId="16aa-3762-4269-e00b::da40e015-a22c-ecc4-c9b8-0add9ff5d50f" book="HH:LACAL" page="30" number="1" type="unit">
          <rules>
            <rule id="0998-96d0-00c0-0e09::8afd-9c24-3738-43b1" name="A Talent for Murder" hidden="false">
