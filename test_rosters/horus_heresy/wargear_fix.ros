            <profile id="f1d0-39a2-0baf-7506::0328-25c5-f8cd-fdad::10b59cf4-3c97-e3e0-2185-853bcde6d112" name="Iron Halo" page="0" hidden="false" typeId="57617267656172204974656d23232344415441232323" typeName="Wargear Item">
