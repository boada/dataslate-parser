                <profile id="2446-35f3-3f1b-e153::642c-8d22-0280-e204::bf7edb98-902e-c8a7-e818-4a6ba05f12f2" name="Rad Grenades" publicationId="cf03-f607-pubN80892" page="84" hidden="false" typeId="57617267656172204974656d23232344415441232323" typeName="Wargear Item">
