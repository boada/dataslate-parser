            <selection id="4699-ec5b-a418-9f05" name="Land Raider Phobos" entryId="bb6a-3566-5621-1916::03bb-fa44-cef6-f91c::a8eda173-f749-cfb6-fd0e-3d6fb35cad3b" entryGroupId="bb6a-3566-5621-1916::b36e-7ece-2ac0-303f" book="HH:LACAL" page="0" number="1" type="upgrade">
