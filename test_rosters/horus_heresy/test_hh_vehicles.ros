                <profile id="194a-0c05-8570-2221::2fe0-ec6e-f4fe-7359::acfd091c-4389-ae8f-3525-a562b3476a12::477d-c630-7e79-8cf9" name="Volkite Serpenta" book="HH: AoD Rulebook" page="178" hidden="false" profileTypeId="576561706f6e23232344415441232323" profileTypeName="Weapon">
