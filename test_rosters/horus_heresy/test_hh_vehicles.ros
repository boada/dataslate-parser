                    <profile id="bb6a-3566-5621-1916::03bb-fa44-cef6-f91c::e51e-6231-64b3-7aba::434e-b473-14ec-4e11::1cce-972c-022a-2590" name="Twin-Linked Lascannon" book="HH: AoD Rulebook" page="178" hidden="false" profileTypeId="576561706f6e23232344415441232323" profileTypeName="Weapon">
