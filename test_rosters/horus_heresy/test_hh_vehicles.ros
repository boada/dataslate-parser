                        <profile id="bb6a-3566-5621-1916::f0df-7af6-dc74-7016::c0ed-4cd4-bdff-8fb6::b4a7-6fba-4cd5-768a::9d51-7566-2ac6-1c08" name="Quad Launcher (Splinter)" book="AL:AoDAL" page="125" hidden="false" profileTypeId="576561706f6e23232344415441232323" profileTypeName="Weapon">
