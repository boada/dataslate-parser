            <rule id="1e32c59e-9fa6-68b0-8aae-f5ad86989520::20f5fdbf-0550-e81f-06b0-075137739092::5ecb-551d-0f68-3a79" name="Implacable Advance" book="HH:GEAFAQV1.0" page="3" hidden="false">
