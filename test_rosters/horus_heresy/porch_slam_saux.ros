                <selection id="87dc-5bdf-05db-d584" name="Plasma + Krak Grenades" entryId="0721-ca72-d85d-be71::c221-cc93-bcfd-b030::42e2-2cc0-6353-4d47" publicationId="a30a-7522-pubN95872" number="1" type="upgrade">
