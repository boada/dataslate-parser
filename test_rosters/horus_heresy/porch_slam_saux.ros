        <selection id="8cf1-7573-6b85-3d6b" name="Auxilia Infantry Tercio" entryId="cae6-1e2b-1637-69d4::25ee-ae11-c0cc-6817" publicationId="7f243fc5--pubN74753" page="264" number="1" type="unit">
