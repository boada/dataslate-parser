            </selection>
            <selection id="1db2-3cf4-6bfb-856f" name="Plasma + Krak Grenades" entryId="7066-ede9-5077-68d6::71d9-af3f-67af-35f6::42e2-2cc0-6353-4d47" publicationId="a30a-7522-pubN95872" number="1" type="upgrade">
              <rules>
                <rule id="7066-ede9-5077-68d6::71d9-af3f-67af-35f6::d109-b31e-e4f6-5b48::0d78-e15c-74f6-5701" name="Plasma Grenades" publicationId="ca571888--pubN105292" hidden="false">
                  <description>Models equipped with plasma grenades don’t suffer the penalty to their Initiative for charging enemies through difficult terrain, but fight at their normal Initiative step in the ensuing combat.</description>
                </rule>
              </rules>
              <profiles>
                <profile id="7066-ede9-5077-68d6::71d9-af3f-67af-35f6::cec2-cbf0-7874-0f36::d9f7-775b-1047-f335" name="Krak Grenade (Shooting)" publicationId="ca571888--pubN106502" page="183" hidden="false" typeId="576561706f6e23232344415441232323" typeName="Weapon">
                  <characteristics>
                    <characteristic name="Range" typeId="52616e676523232344415441232323">8&quot;</characteristic>
                    <characteristic name="Strength" typeId="537472656e67746823232344415441232323">6</characteristic>
                    <characteristic name="AP" typeId="415023232344415441232323">4</characteristic>
                    <characteristic name="Type" typeId="5479706523232344415441232323">Assault 1</characteristic>
                  </characteristics>
                </profile>
