                    <profile id="9940-4829-4689-dcfc::400a-b6ed-c4ed-f452::8c8d-0d8e-df0e-787f::6875-ee73-2a85-6a97" name="Smoke Launchers" hidden="false" profileTypeId="57617267656172204974656d23232344415441232323" profileTypeName="Wargear Item">
