                <profile id="9940-4829-4689-dcfc::543f-e1df-18ef-c256::fb27-514e-2487-c427" name="Unstable Internment" book="HH6: Retribution" page="239" hidden="false" profileTypeId="57617267656172204974656d23232344415441232323" profileTypeName="Wargear Item">
